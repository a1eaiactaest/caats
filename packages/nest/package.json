    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.24",
    "@swc/jest": "^0.2.24",
    "@swc/register": "^0.1.10",
    "@types/cron": "^2.0.0",
    "@types/jest": "28.1.8",
